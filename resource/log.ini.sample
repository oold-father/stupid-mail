[loggers]
keys = root

[formatters]
keys = defaultFormatter

[formatter_defaultFormatter]
format = %(asctime)s %(process)s [%(levelname)s] %(message)s
datefmt = %Y-%m-%d %H:%M:%S
class = logging.Formatter

[handlers]
keys = defaultHandler

[handler_defaultHandler]
class = StreamHandler
level = INFO
formatter = defaultFormatter
args = (sys.stdout,)

[logger_root]
level = INFO
handlers = defaultHandler
qualname = root